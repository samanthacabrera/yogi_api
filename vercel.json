{
  "functions": {
    "app.py": {
      "runtime": "python3.8",
      "maxDuration": 10
    }
  },
  "builds": [
    {
      "src": "api/poses.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/seed.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}




