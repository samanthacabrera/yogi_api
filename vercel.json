{
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "api/poses.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/seed.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/poses",
      "dest": "/api/poses.py"
    },
    {
      "src": "/api/poses/(.*)",
      "dest": "/api/poses.py"
    },
    {
      "src": "/api/seed",
      "dest": "/api/seed.py"
    }
  ]
}
