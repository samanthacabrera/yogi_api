{
  "version": 2,
  "builds": [
    {
      "src": "/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "/dist"
      }
    },
    {
      "src": "server/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/server/(.*)",
      "dest": "/server/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/server/$1"
    }
  ]
}
